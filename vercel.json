{
  "cleanUrls": true,
  "trailingSlash": true,
  "redirects": [
    {
      "source": "/",
      "destination": "/quiz/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/quiz/:path*",
      "destination": "/quiz/:path*"
    },
    {
      "source": "/loja/:path*",
      "destination": "/loja/:path*"
    }
  ]
}